- @power_tweets.each do |tweet|
  -#%li.tweet{:class => @tweet_class}
  %li.tweet
    %span.avatar
      %a{:href => tweet.user_link}
        %img{:src => tweet.profile_image_url, :alt => tweet.username, :height => 48, :width => 48}
